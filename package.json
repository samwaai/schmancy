{
	"name": "@mhmo91/schmancy",
	"version": "0.8.2",
	"description": "UI library build with web components",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./types/src/index.d.ts",
			"default": "./dist/index.js"
		},
		"./mixins": {
			"types": "./types/mixins/index.d.ts",
			"default": "./mixins/index.ts"
		},
		"./ai/*": "./ai/*",
		"./*": {
			"types": "./types/src/*/index.d.ts",
			"default": "./dist/*.js"
		}
	},
	"packageManager": "yarn@4.9.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/mhmo91/schmancy.git"
	},
	"keywords": [
		"lit",
		"UI",
		"Schmancy"
	],
	"type": "module",
	"types": "./types/src/index.d.ts",
	"module": "./dist/index.js",
	"files": [
		"dist",
		"types",
		"mixins",
		"ai",
		"README.md"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"scripts": {
		"dev": "vite build --config vite.config.ts --watch",
		"build": "tsc && vite build --config vite.config.ts",
		"ncu": "ncu -u && npm i",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage"
	},
	"author": "@mhmo91",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/mhmo91/schmancy/issues"
	},
	"homepage": "https://github.com/mhmo91/schmancy#readme",
	"dependencies": {
		"@floating-ui/dom": "^1.7.4",
		"@lit-labs/motion": "^1.0.9",
		"@lit-labs/virtualizer": "^2.1.1",
		"@lit/context": "^1.1.6",
		"@material/material-color-utilities": "^0.3.0",
		"@material/mwc-base": "^0.27.0",
		"@material/web": "^2.4.1",
		"@rollup/plugin-strip": "^3.0.4",
		"@rollup/plugin-terser": "^0.4.4",
		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"@types/cleave.js": "^1.4.12",
		"@webcomponents/scoped-custom-element-registry": "^0.0.10",
		"@webcomponents/webcomponentsjs": "^2.8.0",
		"cleave.js": "^1.6.0",
		"dayjs": "^1.11.19",
		"fastest-levenshtein": "^1.0.16",
		"highlight.js": "^11.11.1",
		"immer": "^10.2.0",
		"jsqr": "^1.4.0",
		"lit": "^3.3.1",
		"playground-elements": "^0.21.2",
		"rxjs": "^7.8.2",
		"tippy.js": "^6.3.7",
		"ts-is-present": "^1.2.2",
		"typeit": "^8.8.7",
		"typescript": "^5.9.3",
		"uuid": "^13.0.0",
		"web-component-analyzer": "^2.0.0"
	},
	"devDependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^24.10.4",
		"@types/uuid": "^11.0.0",
		"@typescript-eslint/parser": "^8.50.0",
		"@vitest/ui": "^4.0.16",
		"autoprefixer": "^10.4.23",
		"c8": "^10.1.3",
		"concurrently": "^9.2.1",
		"deepmerge": "^4.3.1",
		"eslint": "^9.39.2",
		"happy-dom": "^20.0.11",
		"postcss": "^8.5.6",
		"prettier": "^3.7.4",
		"rollup-plugin-copy": "^3.5.0",
		"sass": "^1.97.0",
		"tailwindcss": "^4.1.18",
		"vite": "^7.3.0",
		"vite-plugin-sitemap": "^0.8.2",
		"vite-plugin-webfont-dl": "^3.11.1",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"lit": "^3.2.1",
		"tailwindcss": "^4.0.0"
	}
}
